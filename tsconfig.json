{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"noImplicitAny": false,
		"module": "es6",
		"target": "es5",
		"jsx": "react",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"allowJs": true,
		"strict": false,
		"strictPropertyInitialization": false
	},
	"include": ["./src/**/*"]
}
